open CoqBuildRule
.PHONY: all clean proof

FILES[] =
	Pow
	MultiByte
	ListUtil
	Object
	SerializeSpec
	Prefix
	Soundness
	SerializeImplement
	SerializedList
	DeserializeImplement
	OCamlBase
	Util
	ExtractUtil

.DEFAULT: proof
proof: $(CoqProof $(FILES))

clean:
	rm -rf *.vo *.glob *~ *.omc .omakedb .omakedb.lock
